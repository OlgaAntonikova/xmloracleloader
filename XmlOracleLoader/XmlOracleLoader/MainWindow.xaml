<Window x:Class="XmlOracleLoader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XmlOracleLoader"
        mc:Ignorable="d"
        Title="XML Oracle Sync" Height="250" Width="300">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical" Margin="20" >
        <Button Content="Import" Command="{Binding ImportCommand}" Width="200" Height="40" Margin="5"/>
        <Button Content="Export" Command="{Binding ExportCommand}" Width="200" Height="40" Margin="5"/>
    </StackPanel>
</Window>
